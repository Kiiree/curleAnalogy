FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     centralFoam;

startFrom     	latestTime;

startTime       0;

stopAt          endTime;

endTime         100000;

deltaT          1;

writeControl    timeStep;

writeInterval   10000;

purgeWrite      2;

writeFormat     ascii;

writePrecision  9;

writeCompression yes;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

