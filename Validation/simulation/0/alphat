FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 1e-6;

boundaryField
{
    farfield
    {
        type            calculated;
    }

    cylinder1
    {
        type            zeroGradient;
    }

    cylinder2
    {
        type            zeroGradient;
    }

    periodic1
    {
        type            cyclicAMI;
    }

    periodic2
    {
        type            cyclicAMI;
    }
}


