    readFields
    {
        functionObjectLibs      ("libfieldFunctionObjects.so");

    	type                    readFields;
    	fields                  (U);
    }
    
    Lambda2
    {
    	type                    Lambda2;
    	functionObjectLibs      ("libutilityFunctionObjects.so"); 
    	log                     true;
    	outputControl           timeStep;
    	outputInterval          100;
    }

    magU
    {
        type                    calcMag;
        functionObjectLibs      ("libFVFunctionObjects.so");
        fieldName               "U";
        resultName              "magU";
    	outputInterval          100;
    }
);

